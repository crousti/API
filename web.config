<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        
        <rewrite>
          <rules>
            <rule name="Rewrite to index">
              <match url=".*" />
              <action type="Redirect" url="http://oauth.product-live.com/" />
            </rule>
          </rules>
        </rewrite>
    </system.webServer>
</configuration>